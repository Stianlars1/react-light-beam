{
  "$schema": "https://design-tokens.github.io/schema.json",
  "typography": {
    "font": {
      "family": {
        "sans": {
          "value": "'geistSans', 'geistSans Fallback'",
          "type": "fontFamily"
        }
      }
    },
    "lineHeight": {
      "root": {
        "value": 1.25,
        "type": "lineHeight"
      }
    },
    "outputType": {
      "value": "responsive",
      "type": "string"
    },
    "roles": {
      "display": {
        "size": {
          "value": "clamp(1.974rem, 0.089rem + 8.379vw, 7.629rem)",
          "type": "fontSize"
        }
      },
      "headline": {
        "size": {
          "value": "clamp(1.762rem, 0.315rem + 6.431vw, 6.104rem)",
          "type": "fontSize"
        }
      },
      "title": {
        "size": {
          "value": "clamp(1.574rem, 0.47rem + 4.903vw, 4.883rem)",
          "type": "fontSize"
        }
      },
      "body": {
        "size": {
          "value": "clamp(1rem, 0.667rem + 1.481vw, 2rem)",
          "type": "fontSize"
        }
      },
      "label": {
        "size": {
          "value": "clamp(1.12rem, 0.66rem + 2.044vw, 2.5rem)",
          "type": "fontSize"
        }
      }
    },
    "meta": {
      "viewport": {
        "min": 360,
        "max": 1440
      },
      "responsive": {
        "profile": "product",
        "useCustomRatios": false,
        "mobileRatio": 1.12,
        "desktopRatio": 1.25,
        "optimizeMobile": true,
        "mobileH1Cap": 55
      }
    }
  }
}